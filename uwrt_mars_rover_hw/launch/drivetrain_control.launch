<launch>
    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find uwrt_mars_rover_hw)/config/drivetrain_controllers.yaml" command="load"/>

    <group ns="/uwrt_mars_rover/">
        <node name="uwrt_mars_rover_hw_node" pkg="uwrt_mars_rover_hw" type="node"
              output="screen"/>
        <!-- load the controllers -->
        <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
              output="screen" args="rover_state_controller
                                    drivetrain_velocity_controller"/>

    </group>


</launch>
